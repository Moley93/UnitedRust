{
  "name": "unitedrust-discord-bot",
  "version": "1.0.0",
  "description": "UnitedRust Discord Bot with tickets, curfew, moderation and more",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "node index.js",
    "validate": "node validate-config.js",
    "validate-detailed": "node validate-config.js --show-unused",
    "test-config": "node validate-config.js --test-connections",
    "setup": "node validate-config.js && echo 'Configuration valid! Starting bot...' && node index.js"
  },
  "dependencies": {
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "node-cron": "^3.0.2",
    "express": "^4.18.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "discord",
    "bot",
    "rust",
    "gaming",
    "moderation",
    "unitedrust"
  ],
  "author": "UnitedRust",
  "license": "MIT"
}